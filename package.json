{
  "name": "percy-web",
  "version": "1.0.0",
  "description": "Percy web application",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build": "ember build",
    "build-production": "yarn run clean && ember build --environment=production",
    "clean": "rm -rf dist/ tmp/",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js . --fix",
    "precommit": "lint-staged",
    "postinstall": "bower install --allow-root",
    "start": "ember server",
    "start-production-for-testing": "ember server --environment=production",
    "test": "ember test",
    "test:node_debug": "node debug node_modules/ember-cli/bin/ember test --server",
    "test:parallel": "PERCY_PARALLEL_TOTAL=4 ember exam --split=4 --parallel",
    "test:server": "ember test --server"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/eslint/bin/eslint.js --fix",
      "git add"
    ]
  },
  "repository": "percy/percy-web",
  "author": "Perceptual Inc.",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.6.0",
    "chai-as-promised": "^7.1.1",
    "ember-ajax": "^3.0.0",
    "ember-changeset": "^1.4.1",
    "ember-changeset-validations": "^1.2.9",
    "ember-cli": "^2.16.1",
    "ember-cli-app-version": "^3.1.1",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "^6.10.0",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-clipboard": "^0.8.1",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-flash": "^1.6.1",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-mirage": "0.3.3",
    "ember-cli-mocha": "^0.14.4",
    "ember-cli-moment-shim": "^3.4.0",
    "ember-cli-page-object": "^1.14.0",
    "ember-cli-sass": "^7.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-string-helpers": "^1.7.0",
    "ember-cli-stylelint": "^1.0.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-composable-helpers": "^2.1.0",
    "ember-concurrency": "^0.8.12",
    "ember-data": "2.11.3",
    "ember-data-factory-guy": "^2.13.16",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-fakerjs": "^0.2.0",
    "ember-font-awesome": "^3.0.5",
    "ember-inflector": "^2.0.1",
    "ember-load-initializers": "^1.0.0",
    "ember-modal-dialog": "^2.4.0",

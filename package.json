    "ember-power-select": "^1.10.4",
    "ember-promise-helpers": "^1.0.3",
    "ember-remarkable": "^3.6.1",
    "ember-resolver": "^4.5.0",
    "ember-route-action-helper": "^2.0.6",
    "ember-simple-auth": "^1.4.0",
    "ember-simple-auth-auth0": "git+https://github.com/auth0-community/ember-simple-auth-auth0.git#813fb5b85bba79240621dbf38fe95f33c028ec3d",

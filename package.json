    "loader.js": "^4.6.0",
    "morgan": "^1.9.0",
    "percy-docs": "git+https://github.com/percy/percy-docs.git#082aef8168431fb69038294948adc901b6b68cf2",
    "prettier": "^1.10.0",
    "prettier-eslint-cli": "^4.7.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "engines": {
    "node": ">= 6.9.1"
  },
  "private": true,
  "dependencies": {
    "ember-inline-svg": "^0.1.11",
    "stylelint": "^8.3.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.1.0"
  }
}
